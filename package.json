{
  "name": "ndotoniapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo prebuild --clean",
    "lint": "expo lint",
    "codegen": "graphql-codegen --config codegen.yml",
    "amplify:statements": "amplify codegen statements --nodownload",
    "amplify:types": "amplify codegen types --nodownload",
    "amplify:codegen": "pnpm run amplify:statements && pnpm run amplify:types",
    "schema:download": "aws appsync get-introspection-schema --api-id tpxpbec6e5crxhu277uknqxoqi --region us-west-2 --format SDL schema.graphql",
    "schema:clean": "node scripts/clean-schema.js",
    "schema:generate": "pnpm run schema:clean && pnpm run amplify:codegen",
    "schema:update": "pnpm run schema:download && pnpm run schema:generate",
    "schema:init": "node scripts/init-graphql.js"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.3.3",
    "@aws-amplify/rtn-web-browser": "^1.4.0",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "amazon-cognito-identity-js": "^6.3.16",
    "aws-amplify": "^6.16.0",
    "expo": "~54.0.33",
    "expo-av": "~16.0.8",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-video-thumbnails": "~10.0.8",
    "expo-web-browser": "~15.0.10",
    "fuse.js": "^7.1.0",
    "oidc-client-ts": "^3.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@aws-amplify/cli": "^14.2.5",
    "@aws-amplify/cli-core": "^2.2.3",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/import-types-preset": "^3.0.1",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-operations": "^5.0.8",
    "@types/react": "~19.1.0",
